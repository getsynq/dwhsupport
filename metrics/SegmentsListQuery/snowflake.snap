
[TestSegmentsSuite/TestSegmentQueries/snowflake - 1]

    select to_varchar(workspace) as "segment", to_varchar(run_status) as "segment2", to_varchar(run_type) as "segment3", count(*) as "num_rows"
    from .default.runs 
    where created_at >= '1985-07-16T00:00:00Z' and created_at < '2025-03-16T00:00:00Z' and 1=1
    group by "segment", "segment2", "segment3"
    order by "num_rows" desc limit 10

