
[TestSegmentsSuite/TestSegmentQueries/redshift - 1]

    select CAST(workspace AS VARCHAR) as segment, CAST(run_status AS VARCHAR) as segment2, CAST(run_type AS VARCHAR) as segment3, count(*) as num_rows
    from ""."default"."runs" 
    where created_at >= '1985-07-16T00:00:00Z' and created_at < '2025-03-16T00:00:00Z' and 1=1
    group by segment, segment2, segment3
    order by num_rows desc limit 10

