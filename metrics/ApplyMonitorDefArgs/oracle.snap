
[TestMetricsSuite/TestApplyMonitorDefArgs/oracle_multi_condition_multi_segmentation/no_partitioning - 1]
select
  COALESCE(SUBSTR(TO_CHAR(workspace), 1, 100), '') as "segment", 
  COALESCE(SUBSTR(TO_CHAR(run_status), 1, 100), '') as "segment_2", 
  COALESCE(SUBSTR(TO_CHAR(run_type), 1, 100), '') as "segment_3", 
  'ingested_at' as "field", 
  COUNT(*) as "num_rows", 
  COUNT("ingested_at") as "num_not_null", 
  min("ingested_at") as "min", 
  max("ingested_at") as "max"
from "default"."runs" 
where
  COALESCE(SUBSTR(TO_CHAR(workspace), 1, 100), '') not in ('synq-demo') and 
  COALESCE(SUBSTR(TO_CHAR(run_status), 1, 100), '') in ('1', '2', '3', '4') and 
  (run_status > 0) and (run_type > 0)
group by
  COALESCE(SUBSTR(TO_CHAR(workspace), 1, 100), ''), 
  COALESCE(SUBSTR(TO_CHAR(run_status), 1, 100), ''), 
  COALESCE(SUBSTR(TO_CHAR(run_type), 1, 100), '')
order by
  COALESCE(SUBSTR(TO_CHAR(workspace), 1, 100), ''), 
  COALESCE(SUBSTR(TO_CHAR(run_status), 1, 100), ''), 
  COALESCE(SUBSTR(TO_CHAR(run_type), 1, 100), '') 
---

