
[TestMetricsSuite/TestMultiMetricValues - 1]
select
  count(workspace) as workspace$num_not_null, 
  count(distinct workspace) as workspace$num_unique, 
  countif(workspace = '') as workspace$num_empty, 
  count(run_type) as run_type$num_not_null, 
  count(distinct run_type) as run_type$num_unique, 
  countif(run_type = 0) as run_type$num_empty, 
  CAST(avg(run_type) AS FLOAT64) as run_type$mean, 
  CAST(min(run_type) AS FLOAT64) as run_type$min, 
  CAST(max(run_type) AS FLOAT64) as run_type$max, 
  CAST(approx_quantiles(run_type, 2)[offset(1)] AS FLOAT64) as run_type$median, 
  CAST(stddev_samp(run_type) AS FLOAT64) as run_type$stddev, 
  COALESCE(SAFE_CAST(workspace AS STRING), '') as segment, 
  COALESCE(SAFE_CAST(run_type AS STRING), '') as segment_2
from `db.default.runs` 

group by
  COALESCE(SAFE_CAST(workspace AS STRING), ''), 
  COALESCE(SAFE_CAST(run_type AS STRING), '')
 
---
