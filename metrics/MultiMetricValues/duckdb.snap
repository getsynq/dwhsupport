
[TestMetricsSuite/TestMultiMetricValues - 1]
select
  COALESCE(workspace, '') as segment, 
  COALESCE(run_type, '') as segment_2, 
  count(workspace) as workspace$num_not_null, 
  count(distinct workspace) as workspace$num_unique, 
  SUM(CASE WHEN %s THEN 1 ELSE 0 END)(workspace = '') as workspace$num_empty, 
  COALESCE(count(run_type), 0) as run_type$num_not_null, 
  COALESCE(count(distinct run_type), 0) as run_type$num_unique, 
  COALESCE(SUM(CASE WHEN %s THEN 1 ELSE 0 END)(run_type = 0), 0) as run_type$num_empty, 
  CAST(COALESCE(avg(run_type), 0) AS FLOAT) as run_type$mean, 
  CAST(COALESCE(min(run_type), 0) AS FLOAT) as run_type$min, 
  CAST(COALESCE(max(run_type), 0) AS FLOAT) as run_type$max, 
  CAST(COALESCE(MEDIAN(run_type), 0) AS FLOAT) as run_type$median, 
  CAST(COALESCE(STDDEV(run_type), 0) AS FLOAT) as run_type$stddev
from default.runs 

group by
  segment, 
  segment_2
order by
  segment, 
  segment_2 
---
