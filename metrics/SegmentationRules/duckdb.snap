
[TestMetricsSuite/TestSegmentationRules/duckdb_empty_exclude - 1]
select
  COALESCE(CAST(workspace AS VARCHAR), '') as segment, 
  count(ingested_at) as num_not_null, 
  min(ingested_at) as min, 
  max(ingested_at) as max
from default.runs 

group by segment
order by segment 
