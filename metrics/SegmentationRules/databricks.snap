
[TestMetricsSuite/TestSegmentationRules/databricks_empty_exclude - 1]
select
  COALESCE(CAST(workspace AS STRING), '') as `segment`, 
  count(ingested_at) as `num_not_null`, 
  min(ingested_at) as `min`, 
  max(ingested_at) as `max`
from db.default.runs 

group by `segment`
order by `segment` 
