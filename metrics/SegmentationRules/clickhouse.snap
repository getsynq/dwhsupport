[TestMetricsSuite/TestSegmentationRules/clickhouse_empty_include - 1]
select
  coalesce(toString(workspace), '') as segment, 
  toInt64(count(ingested_at)) as num_not_null, 
  min(ingested_at) as min, 
  max(ingested_at) as max
from default.runs 
where 1=2
group by segment
